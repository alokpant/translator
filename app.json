{
  "expo": {
    "version": "1.0.0",
    "userInterfaceStyle": "light",
    "scheme": "acme",
    "orientation": "portrait",
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      "expo-updates",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 34,
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "name": "translator",
    "slug": "translator",
    "android": {
      "package": "com.anonymous.translator"
    }
  }
}